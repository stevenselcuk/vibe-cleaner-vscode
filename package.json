{
  "name": "tabbycat-vibecleaner",
  "displayName": "vibe-cleaner",
  "icon": "resources/icon.png",
  "description": "Extension that cleans after your AI Assistant. Cleans comments and console.logs",
  "version": "0.0.1",
  "publisher": "Tabby",
  "engines": {
    "vscode": "^1.104.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stevenselcuk/vibe-cleaner-vscode.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.removeComments",
        "title": "Clean Comments"
      },
      {
        "command": "extension.removeConsoleLogs",
        "title": "Clean console.logs"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.34.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}